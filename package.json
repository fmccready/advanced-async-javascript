  "main": "app.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "copy-html": "cp src/*.html dist/",
    "copy-js": "cp ./node_modules/handlebars/dist/handlebars.runtime.js dist/js/handlebars.runtime.js",
    "make-directories": "mkdir -p dist/templates dist/js",
    "build-webpack": "webpack --config webpack.config.js",
    "build-handlebars": "handlebars src/templates/> dist/templates/index.js",
    "start": "npm run make-directories; npm run copy-html; npm run copy-js; npm run build-handlebars; npm run build-webpack & node app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
